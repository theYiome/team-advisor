{
    "name": "team-advisor",
    "productName": "team-advisor",
    "version": "1.0.0",
    "description": "My Electron application description",
    "repository": {
        "type": "git",
        "url": "https://github.com/theYiome/team-advisor.git"
    },
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx ."
    },
    "keywords": [],
    "author": {
        "name": "Kamil Pasterczyk",
        "email": "yiome@outlook.com"
    },
    "license": "",
    "config": {
        "forge": {
            "publishers": [
                {
                    "name": "@electron-forge/publisher-github",
                    "platforms": [
                        "win32"
                    ],
                    "config": {
                        "repository": {
                            "owner": "theYiome",
                            "name": "team-advisor"
                        }
                    }
                }
            ],
            "packagerConfig": {
                "icon": "./src/images/icon"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "team_advisor"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "devContentSecurityPolicy": "default-src 'self' 'unsafe-inline' data:; script-src 'self' 'unsafe-eval'; img-src * data: blob: 'unsafe-inline';",
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/renderer.ts",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@electron-forge/cli": "^6.0.0-beta.63",
        "@electron-forge/maker-deb": "^6.0.0-beta.63",
        "@electron-forge/maker-rpm": "^6.0.0-beta.63",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
        "@electron-forge/maker-zip": "^6.0.0-beta.63",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
        "@electron-forge/publisher-github": "^6.0.0-beta.63",
        "@types/mkdirp": "^1.0.2",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@types/request": "^2.48.8",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "@vercel/webpack-asset-relocator-loader": "^1.7.2",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.7.1",
        "electron": "^18.0.0",
        "eslint": "^8.12.0",
        "eslint-plugin-import": "^2.25.4",
        "fork-ts-checker-webpack-plugin": "^7.2.1",
        "node-loader": "^2.0.0",
        "request": "^2.88.2",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@electron/remote": "^2.0.8",
        "@emotion/react": "^11.8.2",
        "@emotion/styled": "^11.8.1",
        "@fontsource/roboto": "^4.5.5",
        "@mui/icons-material": "^5.5.1",
        "@mui/material": "^5.5.3",
        "auto-launch": "^5.0.5",
        "electron-localshortcut": "^3.2.1",
        "electron-squirrel-startup": "^1.0.0",
        "notistack": "^2.0.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-error-boundary": "^3.1.4",
        "react-qr-code": "^2.0.3",
        "update-electron-app": "^2.0.1"
    }
}
